{
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "eslint:recommended",
        "prettier",
        "plugin:jsx-a11y/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "react/react-in-jsx-scope": 0,
        "react-hooks/rules-of-hooks": "error",
        "no-console": 0,
        "no-undef": 0,
        "no-sequences": 0,
        "no-nested-ternary": 0,
        "no-return-assign": 0,
        "vars-on-top": 0,
        "no-shadow": 0,
        "lines-between-class-members": 0,
        "no-underscore-dangle": 0,
        "react/state-in-constructor": 0,
        "no-plusplus": 0,
        "yoda": 0,
        "indent": 0,
        "linebreak-style": 0,
        "react/prop-types": 0,
        "no-restricted-globals": 0,
        "no-void": 0,
        "prefer-const": 0,
        "no-multi-assign": 0,
        "no-unused-expressions": 0,
        "eqeqeq": 0,
        "no-param-reassign": 0,
        "strict": 0,
        "lines-around-directive": 0,
        "one-var": 0,
        "consistent-return": 0,
        "max-classes-per-file": 0,
        "class-methods-use-this": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "prefer-arrow-callback": 0,
        "no-var": 0,
        "no-bitwise": 0,
        "prefer-template": 0,
        "no-unused-vars": 0,
        "prefer-destructuring": 0,
        "no-use-before-define": 0,
        "no-cond-assign": 0,
        "no-restricted-syntax": 0,
        "no-restricted-properties": 0,
        "no-continue": 0,
        "prefer-object-spread": 0,
        "no-fallthrough": 0,
        "no-empty": 0,
        "default-case": 0,
        "object-shorthand": 0,
        "no-self-assign": 0,
        "new-cap": 0,
        "jsx-a11y/no-onchange": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "prettier/prettier": 0
    },
    "plugins": [
        "prettier",
        "react",
        "react-hooks"
    ]
}